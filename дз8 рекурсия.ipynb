      "source": [
        "def find_max(lst):\n",
        "    if len(lst) == 1:\n",
        "        return lst[0]\n",
        "    else:\n",
        "        return max(lst[0], find_max(lst[1:]))\n",
        "numbers = [3, 7, 2, 9, 5]\n",
        "max_number = find_max(numbers)\n",
        "print(\"Максимальное число:\", max_number)\n"
      ]
    }
